{
  "name": "kb-legal-assistant-ui",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "docx:txt": "node scripts/docx-to-txt.mjs",
    "build-kb": "node scripts/build-kb-from-github.js",
    "kb:check": "node scripts/check-embedding-coverage.js",
    "kb:sidecars": "node scripts/generate-pdf-sidecars.js",
    "kb:sidecars:test": "node scripts/generate-pdf-sidecars.js --limit 5",
    "kb:sidecars:check": "node scripts/generate-pdf-sidecars.js --check",
    "build-kb:dev": "node scripts/build-kb-from-github.js && npm run dev",
    "deploy-kb": "node scripts/deploy-kb.js",
    "fix-pdfs": "node scripts/fix-failed-pdfs.js",
    "install-pdf-tools": "arch -arm64 brew install poppler ghostscript tesseract"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@upstash/redis": "^1.35.4",
    "dotenv": "^17.2.3",
    "mammoth": "^1.6.0",
    "next": "^15.5.4",
    "pdfjs-dist": "^5.4.149",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.14.11",
    "@types/react": "18.3.5",
    "typescript": "5.6.2"
  }
}
